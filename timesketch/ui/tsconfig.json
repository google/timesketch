{
  "compilerOptions": {
    "rootDir": "./timesketch/ui/",
    "baseUrl": "./timesketch/ui/",
    "sourceMap": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "module": "commonjs",
